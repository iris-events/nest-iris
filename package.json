{
  "name": "@iris-events/nestjs-iris",
  "version": "1.0.0-alpha.4",
  "description": "NestJS integration for @iris-events/iris",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "author": "GlobaliD",
  "license": "Apache-2.0",
  "keywords": [
    "nestjs iris iris-events"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iris-events/nest-iris"
  },
  "scripts": {
    "example": "ts-node examples/pingpong.ts",
    "test": "vitest --run",
    "build": "rimraf dist && nest build -p tsconfig.build.json",
    "build:test": "rimraf dist_all_tmp && nest build -p tsconfig.build-test.json && rimraf dist_all_tmp",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --apply ."
  },
  "peerDependencies": {
    "@iris-events/iris": "2.0.0",
    "@nestjs/common": ">=10",
    "@nestjs/core": ">=10",
    "@nestjs/microservices": ">=10",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.1",
    "@nestjs/cli": ">=10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": ">=10",
    "@nestjs/testing": ">=10",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.5.0",
    "@types/amqplib": "^0.10.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "*",
    "@vitest/coverage-v8": "^2.0.1",
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2",
    "unplugin-swc": "^1.4.4",
    "vitest": "^2.0.1"
  },
  "overrides": {
    "reflect-metadata": "^0.2.2"
  },
  "dependencies": {
    "@nestjs/terminus": "^10.2.3",
    "lodash": ">=4.17.21"
  }
}
